{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "rootDir": ".",
    "outDir": "lib",
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "jsx": "react",
    "lib": [
      "es2017",
      "dom",
      "esnext.asynciterable"
    ]
  },
  "files": [
    "typings.d.ts",
    "redux-form.patch.d.ts",
    "src/index.ts",
    "test/tests.ts"
   ]
}
